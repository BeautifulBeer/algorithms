    "def solution(s):\n",
    "    answer = False\n",
    "    count = [0, 0]\n",
    "    for i, vi in enumerate(s):\n",
    "        if vi == 'P' or vi == 'p':\n",
    "            count[0] += 1\n",
    "        elif vi == 'Y' or vi == 'y':\n",
    "            count[1] += 1\n",
    "    if count[0] == count[1]:\n",
    "        answer = True\n",
    "    return answer"
   ]
  }
