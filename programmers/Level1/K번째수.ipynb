    "def solution(array, commands):\n",
    "    answer = []\n",
    "    for _, vi in enumerate(commands):\n",
    "        vi = [v-1 for _, v in enumerate(vi)]\n",
    "        sliced = array[vi[0]:(vi[1]+1)]\n",
    "        sliced.sort()\n",
    "        answer.append(sliced[vi[2]])\n",
    "    return answer"
   ]
  }
